        <tr><td><p>by user: <%= @log.user.username %></p></td></tr>
        <tr><td>Pages: <%= @log.start_page %> to  <%= @log.end_page %></td></tr>
        <tr><td><B>Summary:  </B></td></tr>
        <tr><td><%= @log.summary %></td></tr>
        <tr><td><B>Techniques: </B></td></tr>
        <tr><td><%= @log.techniques %></td></tr>
        <tr><td><B>Characters: </B></td></tr>
        <tr><td><%= @log.characters %></td></tr>
        <tr><td><B>Word Choice: </B></td></tr>
        <tr><td><%= @log.word_choice %></td></tr>
        <tr><td><B>Metacognition: </B></td></tr>
        <tr><td><%= @log.metacognition %></td></tr>
        <tr><td><B>Discussion Question: </B></td></tr>
        <tr><td><%= @log.question %></td></tr>
        <tr>
                <td><a class="button" href="/logs/<%= @log.id %>/edit">Edit Log</a></td>
        </tr>
        <tr>       
                <td>	
        	<form action="/logs/<%=@log.id%>/delete" method="post">
		      <input type="submit" class="button" name="delete" value="Delete Log">
		</form>
	       </td>							
	</tr>        
</table>